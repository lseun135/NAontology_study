@base    <https://lseun135.github.io/NAontology_study/m1-record> .


<https://lseun135.github.io/NAontology_study/m1-record.ttl> a owl:Ontology ;
        owl:imports <https://www.ica.org/standards/RiC/ontology/1.1> , 
                        <http://xmlns.com/foaf/0.1> .


# Class
rico:Agent rdfs:subClassOf foaf:Agent

# individuals
## DateType
nam1:dateTypeScheme a skos:ConceptScheme ;
                skos:prefLabel "Date Type"@en , "날짜 유형"@ko .

nam1:singleDate a rico:DateType, skos:Concept ;
                skos:prefLabel "Single Date"@en, "단일 날짜"@ko ;
                skos:inScheme nam1:dateTypeScheme .

nam1:dateRange a rico:DateType, skos:Concept ;
                skos:prefLabel "Date Range"@en, "날짜 범위"@ko ;
                skos:inScheme nam1:dateTypeScheme ;
                skos:definition "A date that represents a continuous interval with a beginning and an end."@en, 
                        "시작과 끝이 있는 연속 구간을 나타내는 날짜."@ko.

nam1:dateSet a rico:DateType, skos:Concept ;
                skos:prefLabel "Date Set"@en, "날짜 집합"@ko ;
                skos:inScheme nam1:dateTypeScheme .

nam1:year a rico:DateType, skos:Concept ;
                skos:prefLabel "Year"@en, "연도"@ko ;
                skos:inScheme nam1:dateTypeScheme .


## ContentType
nam1:contentTypeScheme a skos:ConceptScheme ;
                skos:prefLabel "Content Type (local controlled list)"@en , "콘텐츠 유형 (내부 통제어휘)"@ko .

nam1:text a rico:ContentType, skos:Concept ;
                skos:prefLabel "text"@en , "텍스트"@ko ;
                skos:inScheme nam1:contentTypeScheme .

nam1:legalStatusScheme a skos:ConceptScheme ;
                skos:prefLabel "Legal Status controlled list"@en , "법적지위 통제어휘"@ko .

nam1:public a rico:LegalStatus, skos:Concept ;
                skos:prefLabel "public record"@en, "공공기록"@ko ;
                skos:inScheme nam1:legalStatusScheme .

nam1:languageTypeScheme a skos:ConceptScheme ;
                skos:prefLabel "language Type"@en , "언어 유형"@ko .

nam1:lang-kor a rico:LanguageType, skos:Concept ;
                skos:prefLabel "Korean"@en , "한국어"@ko ;
                skos:notation "kor" ;
                skos:exactMatch <http://id.loc.gov/vocabulary/iso639-2/kor> ;
                skos:inScheme nam1:languageTypeScheme .



## ActivityType
nam1:NAActivityType rdf:type skos:ConceptScheme ;
                skos:prefLabel "National Assembly Activity Types"@en , "국회 활동 유형"@ko .

nam1:Proceeding rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme  nam1:NAActivityType ;
                skos:topConceptOf nam1:NAActivityType ;  # 나중에 구조 변경될 경우 삭제
                skos:prefLabel "회의"@ko, "Proceeding"@en .

nam1:PlenarySession rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nam1:NAActivityType ;
                skos:broader  nam1:Proceeding ;
                skos:prefLabel "본회의"@ko , "Plenary session"@en ; 
                skos:altLabel  "국회 본회의"@ko .

nam1:SubCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nam1:NAActivityType ;
                skos:broader  nam1:StandingCommitteeMeeting , nam1:SpecialCommitteeMeeting ;
                skos:prefLabel "소위원회 회의"@ko , "Subcommittee meeting"@en .

nam1:StandingCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nam1:NAActivityType ;
                skos:broader  nam1:Proceeding ;
                skos:narrower nam1:SubCommitteeMeeting ;
                skos:prefLabel "상임위원회 회의"@ko , "Standing committee meeting"@en .

nam1:SpecialCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nam1:NAActivityType ;
                skos:broader nam1:Proceeding ;
                skos:narrower nam1:SubCommitteeMeeting ;
                skos:prefLabel "특별위원회 회의"@ko , "Special committee meeting"@en .




## A 분류체계: 국회 회의록·의안기록물

nam1:A rdf:type rico:RecordSet ;
       rico:includesOrIncluded nam1:A-SRG01 ,
                               nam1:A-SRG02 ;
       rico:classification "A" ;
       rico:generalDescription "국회 대수(代數)별로 생산되는 회의록·의안기록물"@ko ;
       rico:name "국회 회의록·의안기록물"@ko ;
       rdfs:label "[A] 국회 회의록·의안기록물"@ko .


nam1:A-SRG01 rdf:type rico:RecordSet ;
                rico:name "국회 회의록"@ko ;
                rdfs:label "[A-SRG01] 국회 회의록"@ko ;
                rico:includesOrIncluded nadat1:S100021.


nam1:A-SRG02 rdf:type rico:RecordSet ;
             rico:name "의안기록물"@ko ;
             rdfs:label "[A-SRG02] 의안기록물"@ko .



## B 분류체계: 생산·이관기록물

nam1:B rdf:type rico:RecordSet ;
       rico:classification "B" ;
       rico:generalDescription "연도별 국회 조직(소속기관 등) 생산 기록물"@ko ;
       rico:name "생산·이관기록물"@ko ;
       rdfs:label "[B] 생산·이관기록물"@ko .



## C 분류체계: 수집기록물

nam1:C rdf:type rico:RecordSet ;
       rico:includesOrIncluded nam1:C-SRG01 ,
                               nam1:C-SRG02 ;
       rico:classification "C" ;
       rico:generalDescription "개인(의원), 단체(정당) 등으로부터 수집한 기록물"@ko ;
       rico:name "수집기록물"@ko ;
       rdfs:label "[C] 수집기록물"@ko .


nam1:C-SRG01 rdf:type rico:RecordSet ;
             rico:name "의원기록"@ko ;
             rdfs:label "[C-SRG01] 의원 기록"@ko .


nam1:C-SRG02 rdf:type rico:RecordSet ;
             rico:name "정당기록"@ko ;
             rdfs:label "[C-SRG02] 정당 기록"@ko .




