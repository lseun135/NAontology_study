@prefix nadat1: <https://lseun135.github.io/NAontology_study/resource/d1-record#> .

@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .

@base    <https://lseun135.github.io/NAontology_study/resource/d1-record> .



<https://lseun135.github.io/NAontology_study/resource/d1-record.ttl> a owl:Ontology .




nadat1:contentTypeScheme a skos:ConceptScheme ;
                skos:prefLabel "Content Type (local controlled list)"@en , "콘텐츠 유형 (내부 통제어휘)"@ko .

nadat1:text a rico:ContentType, skos:Concept ;
                skos:prefLabel "text"@en , "텍스트"@ko ;
                skos:inScheme nadat1:contentTypeScheme .


nadat1:legalStatusScheme a skos:ConceptScheme ;
                skos:prefLabel "Legal Status controlled list"@en , "법적지위 통제어휘"@ko .

nadat1:public a rico:LegalStatus, skos:Concept ;
                skos:prefLabel "public record"@en, "공공기록"@ko ;
                skos:inScheme nadat1:legalStatusScheme .


nadat1:languageTypeScheme a skos:ConceptScheme ;
                skos:prefLabel "language Type"@en , "언어 유형"@ko .

nadat1:lang-kor a rico:LanguageType, skos:Concept ;
                skos:prefLabel "Korean"@en , "한국어"@ko ;
                skos:notation "kor" ;
                skos:exactMatch <http://id.loc.gov/vocabulary/iso639-2/kor> ;
                skos:inScheme nadat1:languageTypeScheme .


## ActivityType

nadat1:NAActivityType rdf:type skos:ConceptScheme ;
                skos:prefLabel "National Assembly Activity Types"@en , "국회 활동 유형"@ko .

nadat1:Proceeding rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme  nadat1:NAActivityType ;
                skos:topConceptOf nadat1:NAActivityType ;  # 나중에 구조 변경될 경우 삭제
                skos:prefLabel "회의"@ko, "Proceeding"@en .

nadat1:PlenarySession rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nadat1:NAActivityType ;
                skos:broader  nadat1:Proceeding ;
                skos:prefLabel "본회의"@ko , "Plenary session"@en ; 
                skos:altLabel  "국회 본회의"@ko .

nadat1:SubCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nadat1:NAActivityType ;
                skos:broader  nadat1:StandingCommitteeMeeting , nadat1:SpecialCommitteeMeeting ;
                skos:prefLabel "소위원회 회의"@ko , "Subcommittee meeting"@en .

nadat1:StandingCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nadat1:NAActivityType ;
                skos:broader  nadat1:Proceeding ;
                skos:narrower nadat1:SubCommitteeMeeting ;
                skos:prefLabel "상임위원회 회의"@ko , "Standing committee meeting"@en .

nadat1:SpecialCommitteeMeeting rdf:type rico:ActivityType , skos:Concept ;
                skos:inScheme nadat1:NAActivityType ;
                skos:broader nadat1:Proceeding ;
                skos:narrower nadat1:SubCommitteeMeeting ;
                skos:prefLabel "특별위원회 회의"@ko , "Special committee meeting"@en .



